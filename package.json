{
  "name": "luniverse-atom-plugin",
  "main": "./lib/luniverse-atom-plugin",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "repository": "https://github.com/Lambda256/luniverse-atom-plugin",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "event-kit": "~1.2.0",
    "highlight.js": "8.x",
    "request": "2.x",
    "shelljs": "^0.8.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "coffeelint": "^1.0.0",
    "coffeescope2": "^0.5.1",
    "husky": "^1.1.3"
  },
  "configSchema": {
    "accountEmail": {
      "title": "Luniverse Email",
      "description": "Your email account at Luniverse. (ex: test@luniverse.io)",
      "type": "string",
      "default": "",
      "order": 1
    },
    "accountPassword": {
      "title": "Luniverse Password",
      "description": "Your password at Luniverse. (ex: p@ssw0rd)",
      "type": "string",
      "default": "",
      "order": 2
    }
  },
  "scripts": {
    "lint": "coffeelint ./lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
